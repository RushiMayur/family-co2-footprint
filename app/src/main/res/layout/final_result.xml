<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/leafy"
    tools:context=".activity.Results">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    <TextView
        android:id="@+id/txtF1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/appbar"
        android:textSize="18sp"
        android:textColor="#ffffff"
        android:textStyle="bold"
        android:layout_marginHorizontal="10dp"
        android:layout_marginTop="8dp"
        android:padding="8dp"
        android:text="Ans"/>

    <TextView
        android:id="@+id/txtF2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:layout_marginHorizontal="10dp"
        android:layout_marginTop="8dp"
        android:padding="8dp"
        android:textStyle="bold"
        android:textColor="#ffffff"
        android:layout_below="@+id/txtF1"
        android:text="Ans"/>

    <TextView
        android:id="@+id/txtF3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:textStyle="bold"
        android:padding="8dp"
        android:layout_marginHorizontal="10dp"
        android:layout_marginTop="8dp"
        android:textColor="#ffffff"
        android:layout_below="@+id/txtF2"
        android:text=" Ans"/>

    <TextView
        android:id="@+id/txtF4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="8dp"
        android:layout_marginHorizontal="10dp"
        android:textSize="18sp"
        android:layout_marginTop="8dp"
        android:textColor="#ffffff"
        android:textStyle="bold"
        android:layout_below="@+id/txtF3"
        android:text=" Ans"/>

    <TextView
        android:id="@+id/txtF5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtF4"
        android:padding="8dp"
        android:textColor="#ffffff"
        android:layout_marginHorizontal="10dp"
        android:layout_marginTop="8dp"
        android:textStyle="bold"
        android:textSize="18sp"
        android:text=" Ans"/>

    <TextView
        android:id="@+id/txtF6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:layout_marginTop="8dp"
        android:textColor="#ffffff"
        android:padding="8dp"
        android:textStyle="bold"
        android:layout_below="@+id/txtF5"
        android:layout_marginHorizontal="10dp"
        android:text=" Ans"/>

    <TextView
        android:id="@+id/txtF7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="8dp"
        android:textSize="18sp"
        android:textColor="#ffffff"
        android:textStyle="bold"
        android:layout_marginTop="8dp"
        android:layout_marginHorizontal="10dp"
        android:layout_below="@+id/txtF6"
        android:text=" Ans"/>

    <TextView
        android:id="@+id/txtF8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="8dp"
        android:layout_below="@+id/txtF7"
        android:textStyle="bold"
        android:layout_marginHorizontal="10dp"
        android:textColor="#ffffff"
        android:textSize="18sp"
        android:layout_marginTop="8dp"
        android:text=" Ans"/>






        <com.google.android.material.appbar.AppBarLayout
            android:id="@+id/appbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:elevation="0dp"
            android:theme="@style/ThemeOverlay.AppCompat.Dark">

            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbar2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#000000"
                android:minHeight="?attr/actionBarSize"
                android:theme="@style/ThemeOverlay.AppCompat.Dark"
                app:layout_scrollFlags="scroll|enterAlways"/>
        </com.google.android.material.appbar.AppBarLayout>

        <FrameLayout
            android:id="@+id/frame"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            app:layout_behavior="@string/appbar_scrolling_view_behavior"/>



    <Button
        android:id="@+id/btnFinalEstimation"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_below="@+id/txtF8"
        android:layout_marginHorizontal="20dp"
        android:text="Net Estimation"
        android:textColor="#ffffff"
        android:layout_marginTop="22dp"
        android:textSize="20dp"
        />
    </RelativeLayout>


</androidx.core.widget.NestedScrollView>